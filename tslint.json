{
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-plugin-prettier", "tslint-config-airbnb", "tslint-react"],
  "rules": {
    "prettier": true,
    "import-name": false,
    "variable-name": false,
    "ter-arrow-parens": false,
    "no-console": false,
    "quotemark": [true, "single", "avoid-escape"],
    "jsx-boolean-value": false,
    "max-line-length": 120
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}